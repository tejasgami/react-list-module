{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "paths": {
      "components/*": ["./src/components/"],
      "assets/*": ["./src/assets/"],
      "pages/*": ["./src/pages/"],
      "store/*": ["./src/store/"],
      "styles/*": ["./src/styles/"],
      "utils/*": ["./src/utils/"]
    }
  },
  "exclude": ["node_modules"],
  "path-intellisense.autoSlashAfterDirectory": true
}
